%p#notice= notice

%p
  %b Username:
  = @user.username
%p
  %b Nickname:
  = @user.nickname
%p
  %b Provider:
  = @user.provider
%p
  %b Url:
  = @user.url
  
%ul
- @friends.each do |friend|
  %li= "#{ friend.first_name } #{ friend.last_name }, Nickname - (#{friend.nickname})"

= link_to 'Back', users_path
